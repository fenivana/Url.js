{
  "name": "browser-url",
  "version": "2.0.0",
  "description": "URL utility for browser",
  "main": "dist/Url.umd.js",
  "module": "dist/Url.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.umd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenivana/browser-url.git"
  },
  "keywords": [
    "url"
  ],
  "author": "Jiang Fengming <fenix@noindoin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fenivana/browser-url/issues"
  },
  "homepage": "https://github.com/fenivana/browser-url#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.1.0",
    "eslint-config-enough": "^0.3.4",
    "rollup": "^0.63.2",
    "rollup-plugin-babel": "^4.0.0-beta.7"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "enough",
    "env": {
      "browser": true
    }
  }
}
